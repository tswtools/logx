{
    "name": "tswtools/logx",
    "description": "Output log to file",
    "license": "MIT",
    "authors": [
        {
            "name": "tswtools",
            "email": "tswtools@163.com",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/tswtools",
    "keywords": ["Laravel", "Logx"],
    "require": {
        "illuminate/support": "5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "^3.5|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Tswtools\\Logx\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tswtools\\Logx\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tswtools\\Logx\\LogxServiceProvider"
            ],
            "aliases": {
                "Logx": "Tswtools\\Logx\\Facades\\Logx"
            }
        }
    }
}
